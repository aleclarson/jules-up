{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "store:default",
    "dialog:default",
    {
      "identifier": "http:default",
      "allow": [
        "https://api.clickup.com/*",
        "https://jules.googleapis.com/*"
      ]
    },
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "git",
          "cmd": "git",
          "args": true
        }
      ]
    },
    "shell:allow-open"
  ]
}
